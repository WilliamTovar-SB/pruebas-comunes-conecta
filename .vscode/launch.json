    "version": "0.2.0",
    "configurations": [
        {
        "type": "aws-sam",
        "request": "direct-invoke",
        "name": "pruebaConcepto local",
        "invokeTarget": {
          "target": "code",
          "projectRoot": "pruebaConceptoGet/",
          "lambdaHandler": "main.handler"
        },
        "lambda": {
          "runtime": "nodejs12.x",
          "payload": {
            "json": {
              "httpMethod": "GET",
              "headers": {
                "Accept": "*/*",
                "Accept-Encoding": "gzip, deflate, br",
                "Cache-Control": "no-cache"
              },
              /*
                "queryStringParameters": {
                  "urlParameter": "53281"
                }
              */
              "data": {
                "idSistemaOrigen": "1",
                "longURL": "https://conecta.segurosbolivar.com/home",
                "diasVigencia": 2
              }
            }
          },
          "environmentVariables": {
            "AWS_REGION": "us-east-1",
            "STACK_ID": "local"
          }
        }
        }
    ]
}